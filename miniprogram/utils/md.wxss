@font-face {
    font-family: 'Material Icons';
    font-style: normal;
    font-weight: 400;
    src: url(https://fonts.gstatic.com/s/materialicons/v117/flUhRq6tzZclQEJ-Vdg-IuiaDsNc.woff2) format('woff2');
  }
  
  .material-icons {
    font-family: 'Material Icons';
    font-weight: normal;
    font-style: normal;
    font-size: 24px;
    line-height: 1;
    letter-spacing: normal;
    text-transform: none;
    display: inline-block;
    white-space: nowrap;
    word-wrap: normal;
    direction: ltr;
    -webkit-font-feature-settings: 'liga';
    -webkit-font-smoothing: antialiased;
  }
  
  .material-icons-add-circle:before {
    content: '\e148';
  }
  
  .material-icons-bolt:before {
    content: '\ea0b';
  }
  
  .material-icons-water-drop:before {
    content: '\e798';
  }
  
  .material-icons-bed:before {
    content: '\efdf';
  }
  
  .material-icons-question-mark:before {
    content: '\eb8b';
  }
  
  .material-icons-navigate-next:before {
    content: '\e409';
  }
  
  .material-icons-edit:before {
    content: '\e3c9';
  }
  
  .material-icons-delete:before {
    content: '\e872';
  }
  
  .icon-water-meter {
    color: #1565c0;
  }
  
  .icon-electric-meter {
    color: #f9a825;
  }
  
  .icon-access-control {
    color: #424242;
  }
  
  .icon-edit {
    color: #616161;
  }
  
  .icon-delete {
    color: #888888;
  }